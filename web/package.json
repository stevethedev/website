{
  "name": "stevethedev-client",
  "description": "Steve the Dev Web Client",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Steven Jimenez",
    "email": "stevenmjimenez@gmail.com"
  },
  "scripts": {
    "build": "npm-run-all check:types build:code",
    "build:code": "tsx scripts/build.ts",
    "build:serve": "npm run build -- --serve",
    "check": "npm-run-all check:*",
    "check:dependencies": "depcheck",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "check:styles": "stylelint --ignore-path .gitignore **/*.css",
    "check:types": "tsc --noEmit",
    "fix": "npm-run-all fix:* check",
    "fix:format": "prettier --write .",
    "fix:lint": "npm run check:lint -- --fix",
    "fix:styles": "npm run check:styles -- --fix",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-a11y": "^8.5.8",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/test": "^8.5.1",
    "@testing-library/react": "^16.2.0",
    "@types/react-dom": "^19.0.4",
    "cmd-ts": "^0.13.0",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "storybook": "^8.5.2",
    "stylelint": "^16.14.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^37.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@std-types/is-number": "^1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "react-router": "^7.1.5"
  }
}
